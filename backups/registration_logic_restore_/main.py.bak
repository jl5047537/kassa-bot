async def check_and_update_level(user_id: str):
    user = db.get_user(user_id)
    if not user:
        return

    current_level = user.get('level', 0)
    referrals_count = db.get_referrals_count(user_id)
    
    # ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼, Ğ´Ğ¾ÑÑ‚Ğ¸Ğ³ Ğ»Ğ¸ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ÑŒ ÑĞ»ĞµĞ´ÑƒÑÑ‰ĞµĞ³Ğ¾ ÑƒÑ€Ğ¾Ğ²Ğ½Ñ
    next_level = current_level + 1
    required_referrals = {
        4: 4,    # ĞĞ°Ñ‡Ğ¸Ğ½Ğ°ÑÑ‰Ğ¸Ğ¹
        3: 16,   # ĞŸÑ€Ğ¾Ğ´Ğ²Ğ¸Ğ½ÑƒÑ‚Ñ‹Ğ¹
        2: 64,   # ĞŸÑ€Ğ¾Ñ„ĞµÑÑĞ¸Ğ¾Ğ½Ğ°Ğ»
        1: 256   # Ğ­ĞºÑĞ¿ĞµÑ€Ñ‚
    }.get(next_level, 0)
    
    if referrals_count >= required_referrals and next_level <= 4:
        db.update_user_level(user_id, next_level)
        return True
    return False

@dp.message_handler(lambda message: message.text == "ğŸ‘¥ ĞŸÑ€Ğ¸Ğ³Ğ»Ğ°ÑĞ¸Ñ‚ÑŒ Ğ”Ñ€ÑƒĞ·ĞµĞ¹")
