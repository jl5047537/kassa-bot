    """
    user_id = str(message.from_user.id)
    user = db.get_user(user_id)
    
    if user:
        # Ğ¤Ğ¾Ñ€Ğ¼Ğ¸Ñ€ÑƒĞµĞ¼ Ğ¸Ğ½Ñ„Ğ¾Ñ€Ğ¼Ğ°Ñ†Ğ¸Ñ Ğ¾ Ğ¿Ñ€Ğ¾Ñ„Ğ¸Ğ»Ğµ
